{
  "name": "jahid-academy-job-site",
  "version": "1.0.0",
  "description": "This is a job portal site where people can search their favorite job, apply, show applied job status etc. There are 3 main section and they are candidate, employer & admin",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "local": "nodemon -r dotenv/config . dotenv_config_path=./.env.local -L ./src/index.js",
    "dev": "nodemon -L ./src/index.js",
    "stage": "node ./src/index.js",
    "start": "node ./src/index.js",
    "docker-compose-up": "docker-compose --env-file ./.env.development -f ./docker-compose.yml -f ./docker-compose.development.yml up --build",
    "docker-compose-down": "docker-compose --env-file ./.env.development -f ./docker-compose.yml -f ./docker-compose.development.yml down"
  },
  "keywords": [
    "job",
    "job site",
    "job portal",
    "job search",
    "job website"
  ],
  "author": "Jahid Hiron",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.525.0",
    "aws-sdk": "^2.1569.0",
    "bullmq": "^5.4.6",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-validator": "^7.0.1",
    "express-xss-sanitizer": "^1.2.0",
    "form-data": "^4.0.0",
    "helmet": "^7.1.0",
    "i18n": "^0.15.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mailgun.js": "^10.2.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "rate-limiter-flexible": "^5.0.0",
    "redis": "^4.6.13",
    "sequelize": "^6.37.1",
    "slugify": "^1.6.6",
    "uuid": "^9.0.1",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-security": "^2.1.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2"
  }
}
